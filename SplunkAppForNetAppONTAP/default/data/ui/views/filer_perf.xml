              <param name="label">View Full Report</param>
            </module>

  <module name="HiddenPostProcess" group="Filer Info"  layoutPanel="panel_row3_col1"  >
                    <param name="groupLabel">Filer Details</param>
                    <param name="search"> stats last(host) as host last(hostname) as hostname last(model) as model  last(version) as version | head 1 </param>
                <module name="SimpleResultsTable" layoutPanel="panel_row2_col1">
                  <param name="displayMenu">true</param>
                  <param name="count">100</param>
                  <param name="drilldown">all</param>
                  <param name="entityName">results</param>
                  <param name="displayRowNumbers">False</param>
   
         
                </module>
     
                  </module>  



	<module name="StaticContentSample" layoutPanel="panel_row3_col1" group="CPU and Latency Statistics">
	<param name="text">CPU and Latency Statistics</param>
  </module>

	<module name="HiddenPostProcess" group="CPU and Latency Statistics" layoutPanel="panel_row3_col1_grp1"  >
                    <param name="search">|timechart min(cpu) as "Min. CPU" avg(cpu) as "Avg. CPU" max(cpu) as "Max. CPU"</param>
                    <module name="HiddenChartFormatter">
                      <param name="chart">line</param>
                      <param name="chart.nullValueMode">connect</param>
                      <param name="legend.placement">bottom</param>
                      <param name="primaryAxisTitle.text">CPU Statistics</param>
                      <module name="FlashChart"></module>
                    </module>
                  </module>


	<module name="HiddenPostProcess" group="CPU and Latency Statistics" layoutPanel="panel_row3_col1_grp2"  >
                    <param name="search">|timechart avg(read_lat) as "Read Latency" avg(write_lat) as "Write Latency" avg(avg_lat) as Latency </param>
                    <module name="HiddenChartFormatter">
                      <param name="chart">line</param>
                      <param name="chart.nullValueMode">connect</param>
                      <param name="legend.placement">bottom</param>
                      <param name="primaryAxisTitle.text">Latency Statistics</param>
                      <module name="FlashChart"></module>
                    </module>
                  </module>



	<module name="HiddenPostProcess" group="Filer Performance Statistics"  layoutPanel="panel_row3_col1"  >
                    <param name="groupLabel">NFS/s</param>
                    <param name="search">| table  cpu read_lat write_lat avg_lat | head 1  </param>
                <module name="SimpleResultsTable" layoutPanel="panel_row3_col1">
                  <param name="displayMenu">true</param>
                  <param name="count">100</param>
                  <param name="drilldown">all</param>
                  <param name="entityName">results</param>
                  <param name="displayRowNumbers">False</param>
   
         
                </module>
     
                  </module>  

</module>

