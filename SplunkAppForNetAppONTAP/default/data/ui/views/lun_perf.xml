<view autoCancelInterval="90" isVisible="true" objectMode="SimpleForm" onunloadCancelJobs="true" template="dashboard.html">
  <label>LUN</label>
  <module name="AccountBar" layoutPanel="appHeader"/>
  <module name="AppBar" layoutPanel="navigationHeader"/>
  <module name="Message" layoutPanel="messaging">
    <param name="filter">*</param>
    <param name="clearOnJobDispatch">False</param>
    <param name="maxSize">1</param>
  </module>
  <module name="Message" layoutPanel="messaging">
    <param name="filter">splunk.search.job</param>
    <param name="clearOnJobDispatch">True</param>
    <param name="maxSize">1</param>
  </module>
  <module name="TitleBar" layoutPanel="viewHeader">
    <param name="actionsMenuFilter">dashboard</param>
  </module>
  
  

    <module name="ExtendedFieldSearch" layoutPanel="viewHeader">
      <param name="label">LUN: </param>
      <param name="field">instance</param>
      <param name="replacementMap">
        <param name="arg">
          <param name="instance">
            <param name="value"/>
          </param>
        </param>
      </param>
      <param name="intention">
        <param name="name">stringreplace</param>
        <param name="arg">
          <param name="instance">
            <param name="default"/>
            <param name="prefix">instance="</param>
            <param name="suffix">"</param>
            <param name="fillOnEmpty">false</param>
          </param>
        </param>
      </param>

 
      <module name="TimeRangePicker">
        <param name="searchWhenChanged">False</param>
        <module name="SubmitButton">
          <param name="allowSoftSubmit">True</param>
          <param name="label">Run Report</param>
  


  


<!--	 START -->





	<module name="HiddenSearch" group="LUN Statistics"  layoutPanel="panel_row2_col1"  >
	<param name="search">index=netapp_perf object=lun $instance$  | stats `snap_avg(avg_latency, total_ops, 2)` AS avg_latency `snap_avg(avg_write_latency, total_ops, 2)` AS write_latency `snap_avg(avg_read_latency, total_ops, 2)` AS read_latency `snap_avg(write_ops, api_time, 2)` AS write_ops `snap_avg(read_ops, api_time, 2)` AS read_ops `snap_avg(other_ops, api_time, 2)` AS other_ops `snap_avg(total_ops, api_time, 2)` AS total_ops  by instance _time</param>

     <module name="HiddenPostProcess" layoutPanel="panel_row2_col1_grp1" group="LUN Statistics">
              <param name="search">|stats  first(instance) as Volume</param>
              <module name="SingleValue">
                <param name="format">count</param>
                <param name="beforeLabel">Stats for LUN: </param>
              </module>
            </module>
   <module name="ViewRedirectorLink" layoutPanel="panel_row2_col1_grp2">
              <param name="viewTarget">flashtimeline</param>
              <param name="label">View Full Report</param>
            </module>


          <module name="HiddenPostProcess" layoutPanel="panel_row4_col1_grp1" group="Latency Info">
              <param name="search">|stats  median("write_latency") as "Write Latency"</param>
              <module name="SingleValue">
                <param name="format">count</param>
                <param name="beforeLabel">Write Latency: </param>
                <param name="afterLabel">ms</param>
              </module>
            </module>

          <module name="HiddenPostProcess" layoutPanel="panel_row4_col1_grp2" group="Latency Info">
              <param name="search">|stats  median("read_latency") as "Read Latency"</param>
              <module name="SingleValue">
                <param name="format">count</param>
                <param name="beforeLabel">Read Latency: </param>
                <param name="afterLabel">ms</param>
              </module>
            </module>

          <module name="HiddenPostProcess" layoutPanel="panel_row4_col1_grp3" group="Latency Info">
              <param name="search">|stats  median("avg_latency") as "Total Latency"</param>
              <module name="SingleValue">
                <param name="format">count</param>
                <param name="beforeLabel">Total Latency: </param>
                <param name="afterLabel">ms</param>
              </module>
            </module>

         
	<module name="HiddenPostProcess" group="Latency Info" layoutPanel="panel_row4_col1"  >
                    <param name="search">|timechart median("write_latency") as "Write Latency" median("read_latency") as "Read Latency"</param>
                    <module name="HiddenChartFormatter">
                      <param name="chart">area</param>
                      <option name="charting.chart.stackMode">stacked100</option>
                      <param name="chart.nullValueMode">connect</param>
                      <param name="legend.placement">bottom</param>
                      <param name="primaryAxisTitle.text">Latency Statistics</param>
                      <module name="JSChart"></module>
                    </module>
                  </module>



       <module name="HiddenPostProcess" layoutPanel="panel_row5_col1_grp1" group="Disk Operations">
              <param name="search">|stats  median("write_ops") as "Write Operations"</param>
              <module name="SingleValue">
                <param name="format">count</param>
                <param name="beforeLabel">Write Operations: </param>

              </module>
            </module>

          <module name="HiddenPostProcess" layoutPanel="panel_row5_col1_grp2" group="Disk Operations">
              <param name="search">|stats  median("read_ops") as "Read Operations"</param>
              <module name="SingleValue">
                <param name="format">count</param>
                <param name="beforeLabel">Read Operations: </param>

              </module>
            </module>

          <module name="HiddenPostProcess" layoutPanel="panel_row5_col1_grp3" group="Disk Operations">
              <param name="search">|stats  median("other_ops") as "Other Operations"</param>
              <module name="SingleValue">
                <param name="format">count</param>
                <param name="beforeLabel">Other Operations: </param>

              </module>
            </module>

         
	<module name="HiddenPostProcess" group="Disk Operations" layoutPanel="panel_row5_col1"  >
                    <param name="search">|timechart median("write_ops") as "Write Ops" median("read_ops") as "Read Ops" median("other_ops") as "Other Ops"</param>
                    <module name="HiddenChartFormatter">
                      <param name="chart">area</param>
                      <option name="charting.chart.stackMode">stacked100</option>
                      <param name="chart.nullValueMode">connect</param>
                      <param name="legend.placement">bottom</param>
                      <param name="primaryAxisTitle.text">Disk Operations</param>
                      <module name="JSChart"></module>
                    </module>
                  </module>




