<view template="dashboard.html" stylesheet="export_button.css">
  <label>Static content</label>
  <module name="AccountBar" layoutPanel="appHeader"/>
  <module name="AppBar" layoutPanel="navigationHeader"/>
  <module name="Message" layoutPanel="messaging">
    <param name="filter">*</param>
    <param name="clearOnJobDispatch">False</param>
    <param name="maxSize">1</param>
  </module>
  <module name="TitleBar" layoutPanel="viewHeader">
    <param name="actionsMenuFilter">dashboard</param>
  </module>


 
  <module name="ServerSideInclude" layoutPanel="panel_row1_col1" group="Common navigation element using ServerSideInclude">
    <param name="src">custom_navigation_example.html</param>
  </module>

  <module name="StaticContentSample" layoutPanel="panel_row2_col1" group="StaticContentSample">
    <param name="text">

      This content is created with a module called StaticContentSample. This is a simple (but limited) way to embed text directly into an advanced XML view.
 
      <![CDATA[
      <p>
      It takes one param &ndash; text.
      </p>
      <p>
        You can specify text encoded in HTML, but you must either place the encoded text within CDATA tags or use HTML entities to escape special characters.
      </p>
      ]]>
    </param>
  </module>


  <module name="ServerSideInclude" layoutPanel="panel_row3_col1" group="ServerSideInclude">
    <param name="src">server_side_include_example.html</param>
  </module>
  
  

  
  
 </view>
